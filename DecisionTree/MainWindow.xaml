            <ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Width="100"/>
            <Label Content="Temperature" VerticalAlignment="Top" HorizontalAlignment="Left" Width="100"/>
            <ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Width="100"/>
            <Label Content="Humidity" VerticalAlignment="Top" HorizontalAlignment="Left" Width="100"/>
            <ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Width="100"/>
            <Label Content="Wind" VerticalAlignment="Top" HorizontalAlignment="Left" Width="100"/>
            <ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Width="100"/>
        </StackPanel>

    </Grid>
</Window>
